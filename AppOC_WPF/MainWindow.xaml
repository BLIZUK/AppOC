<Window x:Name="AppOC" x:Class="AppOC_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppOC_WPF"
        mc:Ignorable="d"
        Title="AppOC" Height="500" Width="850" Icon="/83b1a4442c99450c8753bd0812d511d2.jpg" Background="White">
    <Grid Background="#FFC7EAFF" Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="38*"/>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="38*"/>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="28*"/>
            <RowDefinition Height="43*"/>
            <RowDefinition Height="35*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="6*"/>
            <ColumnDefinition Width="36*"/>
            <ColumnDefinition Width="120*"/>
            <ColumnDefinition Width="34*"/>
            <ColumnDefinition Width="6*"/>
            <ColumnDefinition Width="18*"/>
            <ColumnDefinition Width="230*"/>
            <ColumnDefinition Width="110*"/>
            <ColumnDefinition Width="248*"/>
            <ColumnDefinition Width="28*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Margin="1,2,0,5" Stroke="#FF6C6C6C" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="5" Grid.RowSpan="17"/>
        <Button x:Name="Lb1" Content="Лабораторная работа 1" Margin="13,1,13,1" Click="Lb1_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb2" Content="Лабораторная работа 2" Margin="13,1,13,1" Click="Lb2_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="4" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb3" Content="Лабораторная работа 3" Margin="13,1,13,4" Click="Lb3_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="6" Grid.RowSpan="2" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb4" Content="Лабораторная работа 4" Margin="13,1,13,1" Click="Lb4_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="8" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb5" Content="Лабораторная работа 5" Margin="13,1,13,0" Click="Lb5_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="10" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb6" Content="Лабораторная работа 6" Margin="13,1,13,1" Click="Lb6_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="12" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb7" Content="Лабораторная работа 7" Margin="13,1,13,1" Click="Lb7_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="14" Grid.ColumnSpan="3"/>
        <Button x:Name="Lb8" Content="Лабораторная работа 8" Margin="13,1,13,2" Click="Lb8_Click" FontFamily="Calibri" FontSize="14" Grid.Column="2" Grid.Row="16" Grid.ColumnSpan="3"/>
        <ListBox x:Name="TextInfo" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="2,4,2,5" FontFamily="Calibri" FontSize="14" Grid.Column="7" Grid.Row="1" Grid.ColumnSpan="3" Grid.RowSpan="17"/>
        <Button x:Name="Info" Content="О программе" Margin="3,4,3,4" RenderTransformOrigin="-0.078,-0.379" Click="Info_Click" Background="White" FontFamily="Calibri" FontSize="16" Grid.Column="3" Grid.Row="19"/>
        <Button x:Name="Start" Content="Запуск" Margin="2,4,3,4" Click="Start_Click" FontFamily="Calibri" FontSize="16" Grid.Column="8" Grid.Row="19" Background="#FFB9FF44"/>
        <Label Content="Выбор лабороторных работ:" Margin="4,8,2,0" VerticalAlignment="Top" FontFamily="Calibri" FontSize="14" Grid.Column="2" Height="26" Grid.Row="1" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
